                                    </li>
                                </ul>
                            </div>
                        </nav>

                        <div class="menu-social hidden-xs">
                            <a href="#"><i class="fa fa-facebook"></i></a>
                            <a href="#"><i class="fa fa-twitter"></i></a>
                            <a href="#"><i class="fa fa-instagram"></i></a>
                        </div>
                        <!-- /.menu-social -->

                        <footer class="colophon site-footer hidden-xs">
                            <div class="copyright">
