                                </button>
                                <a class="navbar-brand" href="{{ path('app_homepage') }}"><img src="{{ asset('images/logo.png') }}" alt="Logo"></a>
                            </div>


                            <div class="sidebar-menu collapse navbar-collapse" id="sidebar-menu">
                                <ul class="nav navbar-nav">
                                    <li class="meni-item">
                                        <a href="{{ path('app_homepage') }}">Home</a>
                                    </li>

                                    <li class="meni-item">
                                        <a href="{{ path('app_homepage') }}">About</a>
                                    </li>

                                    <li class="meni-item">
                                        <a href="{{ path('app_homepage') }}">Blog</a>
                                    </li>

                                    <li class="meni-item">
                                        <a href="{{ path('app_homepage') }}">Contact</a>
