        <div class="content-has-sidebar">
            <div class="row">

                <div class="col-sm-2">
                    
                    <aside class="sidebar">
                        <nav class="navbar navbar-default">
                            <div class="navbar-header">
                                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#sidebar-menu" aria-expanded="false">
