                            </div>
                            <!-- /.copyright -->
                        </footer>
                        <!-- /.site-footer -->
                    </aside>
                    <!-- /.sidebar -->
                </div>

                <div class="col-sm-10">
                    
                    {% block body %}{% endblock %}

                    <footer class="colophon site-footer visible-xs text-center">
                        <div class="footer-social">
                            <a href="#"><i class="fa fa-facebook"></i></a>
                            <a href="#"><i class="fa fa-twitter"></i></a>
                            <a href="#"><i class="fa fa-instagram"></i></a>
                        </div>
                        <!-- /.footer-social -->

                        <div class="copyright">
                            2017 © <a href="http://demos.jeweltheme.com/owlfolio">Owlfolio</a> Developed by <a href="https://jeweltheme.com">Jeweltheme</a>
